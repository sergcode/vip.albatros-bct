mixin petersburg-fotorama
  - var wayDirectory = 'Petersburg/Mercedes-Benz_'

  #petersburgFotorama.tab-pane.fade(role="tabpanel" aria-labelledby="petersburgSelectTab")

    ul.nav-pills
      li.dropdown
        a.dropdown-toggle(data-toggle='dropdown' href='#' role='button' aria-haspopup='true' aria-expanded='false') Выберите город
        .dropdown-menu
          .nav.nav-tabs(role="tablist")
            a#moscowSelectTab(role="tab" data-toggle='tab' href='#moscowFotorama' aria-controls="moscowFotorama" aria-selected="false") Москва
            a#petersburgSelectTab.active(role="tab" data-toggle='tab' href='#petersburgFotorama' aria-controls="petersburgFotorama" aria-selected="true") Санкт-Петербург
            a#sochiSelectTab(role="tab" data-toggle='tab' href='#sochiFotorama' aria-controls="sochiFotorama" aria-selected="false") Сочи
            a#novosibirskSelectTab(role="tab" data-toggle='tab' href='#novosibirskFotorama' aria-controls="novosibirskFotorama" aria-selected="false") Новосибирск
            a#vladivostokSelectTab(role="tab" data-toggle='tab' href='#vladivostokFotorama' aria-controls="vladivostokFotorama" aria-selected="false") Владивосток

      li.dropdown
        a.dropdown-toggle(data-toggle='dropdown' href='#' role='button' aria-haspopup='true' aria-expanded='false') Выберите класс авто
        .dropdown-menu
          .nav.nav-tabs(role="tablist")
            a#sClassW222MaybachPet.active(role="tab" data-toggle='tab' href='#mercedesSclassW222MaybachPet' aria-controls="mercedesSclassW222MaybachPet" aria-selected="true") Mercedes-Benz S class W222 Maybach
            a#sClassW222Pet(role="tab" data-toggle='tab' href='#mercedesSclassW222Pet' aria-controls="mercedesSclassW222Pet" aria-selected="false") Mercedes-Benz S class W222
            a#eClassW213Pet(role="tab" data-toggle='tab' href='#mercedesEclassW213Pet' aria-controls="mercedesEclassW213Pet" aria-selected="false") Mercedes-Benz E class W213
            a#vClassPet(role="tab" data-toggle='tab' href='#mercedesVclassPet' aria-controls="mercedesVclassPet" aria-selected="false") Mercedes-Benz V class

    .tab-content
      #mercedesSclassW222MaybachPet.tab-pane.fade.show.active(role="tabpanel" aria-labelledby="sClassW222MaybachPet")
        .fotorama.fotorama__tariffs-political(data-auto="false" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30')
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/1.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/2.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/3.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/4.jpg" data-caption="Mercedes-Benz S class W222 Maybach")

        .tariffs-political__fotorama_info
          p
            | Доступно для заказа:
            br
            | 27 автомобилей
          p
            | Год выпуска:
            br
            | 2019 г. | 2018 г. | 2017 г.
          ul
            li AMG пакет
            li ЖК панель
            li Панорамная крыша

      #mercedesSclassW222Pet.tab-pane.fade(role="tabpanel" aria-labelledby="sClassW222Pet")
        .fotorama.fotorama__tariffs-political(data-auto="false" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30')
