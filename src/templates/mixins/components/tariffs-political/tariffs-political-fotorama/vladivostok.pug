mixin vladivostok-fotorama
  - var wayDirectory = 'Vladivostok/Mercedes-Benz_'

  #vladivostokFotorama.tab-pane.fade(role="tabpanel" aria-labelledby="vladivostokSelectTab")

    ul.nav-pills
      li.dropdown
        a.dropdown-toggle(data-toggle='dropdown' href='#' role='button' aria-haspopup='true' aria-expanded='false') Выберите город
        .dropdown-menu
          .nav.nav-tabs(role="tablist")
            a#moscowSelectTab(role="tab" data-toggle='tab' href='#moscowFotorama' aria-controls="moscowFotorama" aria-selected="false") Москва
            a#petersburgSelectTab(role="tab" data-toggle='tab' href='#petersburgFotorama' aria-controls="petersburgFotorama" aria-selected="false") Санкт-Петербург
            a#sochiSelectTab(role="tab" data-toggle='tab' href='#sochiFotorama' aria-controls="sochiFotorama" aria-selected="false") Сочи
            a#novosibirskSelectTab(role="tab" data-toggle='tab' href='#novosibirskFotorama' aria-controls="novosibirskFotorama" aria-selected="false") Новосибирск
            a#vladivostokSelectTab.active(role="tab" data-toggle='tab' href='#vladivostokFotorama' aria-controls="vladivostokFotorama" aria-selected="true") Владивосток

      li.dropdown
        a.dropdown-toggle(data-toggle='dropdown' href='#' role='button' aria-haspopup='true' aria-expanded='false') Выберите класс авто
        .dropdown-menu
          .nav.nav-tabs(role="tablist")
            a#sClassW222MaybachVlad.active(role="tab" data-toggle='tab' href='#mercedesSclassW222MaybachVlad' aria-controls="mercedesSclassW222MaybachVlad" aria-selected="true") Mercedes-Benz S class W222 Maybach
            a#sClassW222Vlad(role="tab" data-toggle='tab' href='#mercedesSclassW222Vlad' aria-controls="mercedesSclassW222Vlad" aria-selected="false") Mercedes-Benz S class W222
            a#eClassW213Vlad(role="tab" data-toggle='tab' href='#mercedesEclassW213Vlad' aria-controls="mercedesEclassW213Vlad" aria-selected="false") Mercedes-Benz E class W213
            a#vClassVlad(role="tab" data-toggle='tab' href='#mercedesVclassVlad' aria-controls="mercedesVclassVlad" aria-selected="false") Mercedes-Benz V class


    .tab-content
      #mercedesSclassW222MaybachVlad.tab-pane.fade.show.active(role="tabpanel" aria-labelledby="sClassW222MaybachVlad")
        .fotorama.fotorama__tariffs-political(data-auto="false" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30')
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/1.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/2.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/3.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/4.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
          img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/5.jpg" data-caption="Mercedes-Benz S class W222 Maybach")

        .tariffs-political__fotorama_info
          p
            | Доступно для заказа:
            br
            | 27 автомобилей
          p
            | Год выпуска:
            br
            | 2019 г. | 2018 г. | 2017 г.
          ul
            li AMG пакет
            li ЖК панель
            li Панорамная крыша

      #mercedesSclassW222Vlad.tab-pane.fade(role="tabpanel" aria-labelledby="sClassW222Vlad")
