mixin vladivostok-tabs
  - var wayDirectory = 'Vladivostok/Mercedes-Benz_'

  #vladivostok.tab-pane.tariffs-political__tab-pane.fade(role="tabpanel" aria-labelledby="vladivostok-tab")
    .tab-content.tariffs-political__carousel-wrapper
      #MercedesBenzSclassW222MaybachVlad.tab-pane.fotorama.fotorama__tariffs-political.fade.show.active(data-auto="false" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30' role="tabpanel" aria-labelledby="vladivostokHeadingOne")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/1.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/2.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/3.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/4.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/5.jpg" data-caption="Mercedes-Benz S class W222 Maybach")

      #MercedesBenzSclassW222Vlad.tab-pane.fotorama.fotorama__tariffs-political.fade(data-auto="false" role="tabpanel" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30' aria-labelledby="vladivostokHeadingTwo")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222/1.jpg" data-caption="Mercedes-Benz S class W222")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222/2.jpg" data-caption="Mercedes-Benz S class W222")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222/3.jpg" data-caption="Mercedes-Benz S class W222")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222/4.jpg" data-caption="Mercedes-Benz S class W222")

      #MercedesBenzEclassW213Vlad.tab-pane.fotorama.fotorama__tariffs-political.fade(data-auto="false" role="tabpanel" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30' aria-labelledby="vladivostokHeadingThree")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "E_class_W213/1.jpg" data-caption="Mercedes-Benz E class W213")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "E_class_W213/2.jpg" data-caption="Mercedes-Benz E class W213")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "E_class_W213/3.jpg" data-caption="Mercedes-Benz E class W213")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "E_class_W213/4.jpg" data-caption="Mercedes-Benz E class W213")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "E_class_W213/5.jpg" data-caption="Mercedes-Benz E class W213")

      #MercedesBenzVclassVlad.tab-pane.fotorama.fotorama__tariffs-political.fade(data-auto="false" role="tabpanel" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30' aria-labelledby="vladivostokHeadingFour")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/1.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/2.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/3.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/4.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/5.jpg" data-caption="Mercedes-Benz V class")

    .tariffs-political__information
      ul#accordionTariffsPoliticalVladivostok.nav.tariffs-political__accordion(role="tablist")

        li.nav-item.tariffs-political__card
          a#vladivostokHeadingOne.nav-link.tariffs-political__card-header.active(data-toggle="tab" href="#MercedesBenzSclassW222MaybachVlad" role="tab" aria-controls="MercedesBenzSclassW222MaybachVlad" aria-selected="true")
            h2
              button(type="button" data-toggle="collapse" data-target="#vladivostokCollapseOne" aria-expanded="true" aria-controls="vladivostokCollapseOne")
                | Mercedes-Benz S class W222 Maybach
          #vladivostokCollapseOne.collapse.show.tariffs-political__collapse(aria-labelledby="vladivostokHeadingOne" data-parent="#accordionTariffsPoliticalVladivostok")
            p Доступно для заказа: 27 автомобилей
            p Год выпуска: 2019 г. | 2018 г. | 2017 г.
            ul
              li AMG пакет
              li ЖК панель
              li Панорамная крыша

        li.nav-item.tariffs-political__card
          a#vladivostokHeadingTwo.nav-link.tariffs-political__card-header(data-toggle="tab" href="#MercedesBenzSclassW222Vlad" role="tab" aria-controls="MercedesBenzSclassW222Vlad" aria-selected="false")
