mixin sochi-tabs
  - var wayDirectory = 'Sochi/Mercedes-Benz_'

  #sochi.tab-pane.tariffs-political__tab-pane.fade(role="tabpanel" aria-labelledby="sochi-tab")
    .tab-content.tariffs-political__carousel-wrapper
      #MercedesBenzSclassW222MaybachSochi.tab-pane.fotorama.fotorama__tariffs-political.fade.show.active(data-auto="false" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30' role="tabpanel" aria-labelledby="sochiHeadingOne")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/1.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/2.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/3.jpg" data-caption="Mercedes-Benz S class W222 Maybach")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "S_class_W222_Maybach/4.jpg" data-caption="Mercedes-Benz S class W222 Maybach")

      #MercedesBenzSclassW222Sochi.tab-pane.fotorama.fotorama__tariffs-political.fade(data-auto="false" role="tabpanel" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30' aria-labelledby="sochiHeadingTwo")
