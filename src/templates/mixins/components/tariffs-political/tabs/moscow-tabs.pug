      #MercedesBenzVclass.tab-pane.fotorama.fotorama__tariffs-political.fade(data-auto="false" role="tabpanel" data-thumbwidth="101" data-thumbheight="101" data-thumbmargin='30'  aria-labelledby="moscowHeadingFour")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/1.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/2.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/4.jpg" data-caption="Mercedes-Benz V class")
        img(src=pathAssetsImg + "tariffs-political/" + wayDirectory + "V_class/5.jpg" data-caption="Mercedes-Benz V class")

    .tariffs-political__information
      ul#accordionTariffsPoliticalMoscow.nav.tariffs-political__accordion(role="tablist")

        li.nav-item.tariffs-political__card
          a#moscowHeadingOne.nav-link.tariffs-political__card-header.active(data-toggle="tab" href="#MercedesBenzSclassW222Maybach" role="tab" aria-controls="MercedesBenzSclassW222Maybach" aria-selected="true")
            h2
              button(type="button" data-toggle="collapse" data-target="#moscowCollapseOne" aria-expanded="true" aria-controls="moscowCollapseOne" aria-selected="true")
                | Mercedes-Benz S class W222 Maybach
          #moscowCollapseOne.collapse.show.tariffs-political__collapse(aria-labelledby="moscowHeadingOne" data-parent="#accordionTariffsPoliticalMoscow")
            p Доступно для заказа: 27 автомобилей
            p Год выпуска: 2019 г. | 2018 г. | 2017 г.
            ul
              li AMG пакет
              li ЖК панель
              li Панорамная крыша

        li.nav-item.tariffs-political__card
